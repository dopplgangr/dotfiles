dotfiles::compinit::is_stale() {
  local compfile=~/.zplug/zcompdump
  if [[ -f $compfile ]] || return 1

  local timeo=$1
  local CURTIME=$(date +'%s')
  local FILETIME=$(stat --format '%Y' "$compfile")
  local TIMEDIFF=$(expr $CURTIME - $FILETIME)
  if [[ $TIMEDIFF -gt $timeo ]] && return 0 || return 1
}
